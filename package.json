{
  "name": "eslint-plugin-eslint-react-query",
  "version": "0.0.0",
  "description": "React Query checks",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "dist/index.js",
  "types": "index.d.ts",
  "author": "Matias HernÃ¡ndez",
  "dependencies": {
    "@typescript-eslint/scope-manager": "5.40.1",
    "@typescript-eslint/utils": "5.40.1",
    "eslint-module-utils": "2.7.4",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@jest/types": "29.2.1",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "5.40.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "8.0.1",
    "jest": "29.2.1",
    "lint-staged": "13.0.3",
    "mocha": "^10.0.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "*"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "license": "ISC",
  "files": [
    "dist",
    "pacakage.json",
    "README.md"
  ],
  "lint-staged": {
    "*.{ts,js,css,md}": "prettier --write",
    "*.ts": [
      "prettier --parser typescript --write",
      "npx eslint -c .eslintrc.js --ext .ts --max-warnings 0"
    ]
  },
  "scripts": {
    "clean":"rimraf ./dist",
    "build": "npm run clean && mkdir ./dist && tsc --project tsconfig.build.json",
    "lint": "npx eslint -c .eslintrc.js --ext .ts './lib/**/*.ts' './tests/**/*.ts' --no-error-on-unmatched-pattern",
    "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --silent=false -c jest.config.ts",
    "pre-commit": "npx lint-staged",
    "prettier": "prettier --write lib/*",
    "prepare": "husky install"
  }
}
